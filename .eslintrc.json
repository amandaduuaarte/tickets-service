{
     "env": {
         "es2021": true,
         "node": true
     },
     "extends": "airbnb-base",
     "parserOptions": {
         "ecmaVersion": "latest",
         "sourceType": "module"
     },
     "rules": {
         "semi": ["error", "always"],
         "quotes": ["error", "double"],
         "prefer-destructuring": 0,
         "no-console": 0,
         "class-methods-use-this": 0,
         "import/extensions": 0
     }
 }